<Window x:Class="Homework4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBlock Grid.Row ="0" Grid.Column="0">Zip Code</TextBlock>
        <TextBox x:Name="uxTexBox" Grid.Row ="0" Grid.Column="2" TextChanged="uxTexBox_TextChanged"></TextBox>
        <Button x:Name="uxButton" 
                Click="Button_Loaded"
                IsEnabled="False">Submit</Button>
    </StackPanel>
</Window>
